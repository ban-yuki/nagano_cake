<h2>会員一覧</h2>
<table>
  <tr>
    <th>会員ID</th>
    <th>氏名</th>
    <th>メールアドレス</th>
    <th>会員ステータス</th>
  </tr>
  <% @customers.each do |customer| %>
    <tr>
      <td><%= customer.id %></td>
      <td><%= link_to admin_item_path(item.id) do %>
            <%= item.name %>
          <% end %>
      </td>
      <td><%= item.price %></td>
    </tr>
  <% end %>
</table>